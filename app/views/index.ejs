    <div class="container-fluid">
      <div class="row">
        <div class="container-fluid main">
          <h1 class="page-header">Dashboard</h1>

          <div class="row placeholders">
              <script>
              jQuery(document).ready(function($) {
                $('#slider').click(function() {
                  writeTempPrefData();
                })
              });
              </script>     
          </div>


<!--
By accessing and/or using this code snippet, you agree to AccuWeather’s terms and conditions (in English) which can be found at https://www.accuweather.com/en/free-weather-widgets/terms and AccuWeather’s Privacy Statement (in English) which can be found at https://www.accuweather.com/en/privacy.
-->

          <!--<h2 class="sub-header">Settings</h2>-->
          <div class="row placeholders">
           <div class="col-xs-6 col-sm-3 placeholder">
                <% if (weather == "Overcast" || weather == "Partly Cloudy") {%>
                  <div class="temp outside-temp"><i class='fa fa-cloud' aria-hidden='true'></i> <%=String(parseInt(temp_f)) + "°F" %></div>
                <%} else if (weather == "Rain") {%>
                  <div class="temp outside-temp"><i class='fa fa-tint' aria-hidden='true'></i> <%=String(parseInt(temp_f)) + "°F" %></div>
                <%} else if (weather == "Clear") {%>
                <div class="temp outside-temp"><i class='fa fa-sun-o' aria-hidden='true'></i> <%=String(parseInt(temp_f)) + "°F" %></div>
               <% }%>
            
                <p style="text-align: center"><h4>Current weather</h4></p>
              <div class="location"><%=location%></div>
             </div>
          <!-- Range slider, low and high tracks, and selection: -->
          <!-- Adapted from : https://stackoverflow.com/questions/4753946/html5-slider-with-two-inputs-possible -->
        <div class="col-xs-6 col-sm-3 placeholder">
          <div class="temp">
            <i class="fa fa-thermometer-half" aria-hidden="true"></i>
            <input type="number" id="temp-input" name="quantity" min="1" max="10" step="1" value="<%= temp %>">
          </div>
        <span class="temp-pref">
         <p><h4>Jacket preferred temperature</h4></p>
         </span>
        </div>
        <div class="col-xs-6 col-sm-3 placeholder" style="padding-top: 15px">
          <br>
            <!--<h3>Gestures</h3>-->
            <label class="switch">
              <% if (gestures == 'true' || gestures == 'True') {%>
                <input type="checkbox" id="gest_switch" checked>
              <% } else { %>
                <input type="checkbox" id="gest_switch" >
               <% } %>
              <span class="slider round"></span>
            </label>
            <br>
            <br>

            <p style="text-align: center"><h4>Gestures</h4></p>
         
        </div>

        </div>
          <h2 class="sub-header">Sensor readings</h2>
          <div class="temp_f">
            <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/376074/charts/1?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=20&title=Temperature+sensor+%28left%29&type=line&xaxis=Time&yaxis=Temperature+%28degrees+F%29"></iframe>
            <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/376074/charts/2?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=20&title=Temperature+sensor+%28right%29&type=line&xaxis=Time&yaxis=Temperature+%28degrees+F%29"></iframe>
          </div>
          <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/376074/charts/5?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=20&title=Pressure+sensor+%28left%29&type=line&xaxis=Time&yaxis=Pressure+%28Pa%29"></iframe>
          <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/376074/charts/6?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=20&title=Pressure+sensor+%28right%29&type=line&xaxis=Time&yaxis=Pressure+%28Pa%29"></iframe>
        </div>
      </div>
    </div>

